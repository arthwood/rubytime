.datepicker_calendar{:style => 'display: none'}
  .nav
    = link_to image_tag('datepicker/arrow_left.gif'), '#'
    = select_month Time.now
    = select_year Time.now
    = link_to image_tag('datepicker/arrow_right.gif'), '#'
  %table
    %tr
      - 1.upto(7) do |j|
        %th &nbsp;
    - 1.upto(5) do |i|
      %tr
        - 1.upto(7) do |j|
          %td &nbsp;
